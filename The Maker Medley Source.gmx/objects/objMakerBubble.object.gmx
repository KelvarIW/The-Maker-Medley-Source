            <string>if (instance_exists(objPlayer)){
    //Visual
    var _index = (floor(global.count * 4/50) mod 4);
    if (_index &lt; 3)
        image_index = _index;
    else
        image_index = 1;
    
    //Movement
    var _mult, xMove, yMove;
    if (scrButtonCheck(global.jumpButton)){
        _mult = 0.5;
    }else{
        _mult = 1;
    }
    if (scrButtonCheck(global.rightButton)){
        xMove = 4*_mult;
    }else if (scrButtonCheck(global.leftButton)){
        xMove =-4*_mult;
    }else{
        xMove = 0;
    }
    
    if (scrButtonCheck(global.downButton)){
        yMove = 4*_mult;
    }else if (scrButtonCheck(global.upButton)){
        yMove =-4*_mult;
    }else{
        yMove = 0;
    }
    
    with (objPlayer){
        if (!place_meeting(x+xMove, y+yMove, objBlock)){
            x += xMove;
            y += yMove;
        }else{
            scrMoveContactObject(sign(xMove), 0, abs(xMove), objBlock);
            scrMoveContactObject(0, sign(yMove), abs(yMove), objBlock);
        }
        other.x = roundSane(x);
        other.y = roundSane(y);
        sprite_index = sprPlayerFall;
        speed = 0;
        image_index += (0.5 - image_speed);
    }
}
