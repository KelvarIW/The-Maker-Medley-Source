            <kind>1</kind>
            <string>if (!collected){
    if (index == 1){
        if (pvzAnimTimer == -1){
            if (y &gt;= ystart &amp;&amp; vspeed &gt; 0){
                if (vspeed &gt; 3){
                    vspeed *= -4/5;
                    y -= 2*(y - ystart);
                    audio_sound_pitch(audio_play_sound(sndMakerCoin, 0, 0), 1.4);
                }else{
                    vspeed = 0;
                    y = ystart;
                }
            }
            if (mouse_check_button_pressed(mb_left) &amp;&amp; place_meeting(x, y, objMouseClickCheck)){
                pvzAnimTimer = 0;
                xstart = x;
                ystart = y;
                
                with (instance_create(x, y, objScreenFade)){
                    color = c_white;
                    fadeIn = 1/160;
                    depth = other.depth + 1;
                }
                
                // automatically collect all coins,  consistent with PvZ
                with (objPlantsCoin){
                    with (instance_create(x, y, objPlantsCoinCollected)){
                        image_index = other.image_index;
                        brightness = other.brightness;
                        yBase = other.yBase;
                        
                        scaleInit = other.image_xscale;
                        image_xscale = scaleInit;
                        image_yscale = image_xscale;
                    }
                    instance_destroy();
                }
                
                audio_play_sound(sndPlantsVictory, 0, 0);
            }
        }
        if (pvzAnimTimer &gt;= 0){
            pvzAnimTimer ++;
            if (pvzAnimTimer &lt; 160){
                x = lerp(xstart, 400, pvzAnimTimer/160);
                y = lerp(ystart, 304, pvzAnimTimer/160);
                image_xscale = lerp(2, 4, pvzAnimTimer/160);
                image_yscale = lerp(2, 4, pvzAnimTimer/160);
            }else if (pvzAnimTimer == 160){
                x = 400;
                y = 304;
                event_user(0);
                with (objPlantsCtrl)
                    alarm[1] = 40;
            }
        }
