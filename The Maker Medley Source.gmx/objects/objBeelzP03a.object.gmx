        _proj.image_index = 5*floor(indexNext);
        indexBase = 5*floor(indexNext);
        image_index = indexBase + (image_index mod 5);
        
        //set next index
        indexNext += indexDir/2;
        if (indexNext &gt; 6){
            indexDir = -1;
            indexNext = 6;
        }else if (indexNext &lt; 0){
            indexDir = 1;
            indexNext = 1;
        }
        shootDelay += 3;
    }
    image_angle += _inc * global.deathSlow;
}
