    if (buff()){
        _inc = 1;
    }else{
        _inc = 0.6;
    }
    
    shootDelay -= global.deathSlow;
    if (shootDelay &lt;= 0){
        var _proj;
        _proj = instance_create(x + lengthdir_x(42, image_angle), y + lengthdir_y(42, image_angle), objMakerTHBullet);
        _proj.speed = 5*global.deathSlow;
        _proj.sprite_index = sprMakerTHPointed;
        _proj.direction = image_angle;
